name: 京东比价/返利
desc: 更优雅的显示返利与历史最低价格

http:
  mitm:
    - "buy.itunes.apple.com"
  script:
    - match: ^https?://api\.m\.jd\.com/(client\.action|api)\?functionId=(wareBusiness|serverConfig|basicConfig|lite_wareBusiness|pingou_item)
      name: jdjf_price
      type: response
      require-body: true
      timeout: 10
      
      
script-providers:
  jdjf_price:
    url: https://raw.githubusercontent.com/Tartarus2014/Script/master/jdjf_price.js
    interval: 3600